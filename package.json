{
    "name": "stbchef",
    "version": "1.0.0",
    "description": "Simple Crypto Trading Bot Chef - Compare AI Models in Crypto Trading",
    "main": "src/web/app.py",
    "scripts": {
        "start": "python -m src.web.app",
        "dev": "nodemon --exec python -m src.web.app",
        "compile": "npx hardhat compile",
        "deploy": "npx hardhat run scripts/deploy.js --network lineaTestnet",
        "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
        "node": "npx hardhat node",
        "update-addresses": "node scripts/update-contract-addresses.js",
        "deploy-and-update": "npm run deploy | tee deployment.log && node scripts/extract-addresses.js && npm run start",
        "update-frontend-addresses": "node scripts/update-frontend-addresses.js",
        "install-deps": "npm install --legacy-peer-deps",
        "test:e2e": "playwright test --config=playwright.config.js"
    },
    "dependencies": {
        "@metamask/sdk": "^0.5.6",
        "@playwright/test": "^1.43.0",
        "ethers": "^5.7.2",
        "web3": "^1.10.0"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
        "@nomicfoundation/hardhat-toolbox": "^2.0.2",
        "@nomiclabs/hardhat-ethers": "^2.2.3",
        "@nomiclabs/hardhat-etherscan": "^3.1.7",
        "@openzeppelin/contracts": "^4.9.5",
        "@typechain/ethers-v5": "^10.2.1",
        "@typechain/hardhat": "^6.1.6",
        "@types/chai": "^4.3.20",
        "@types/mocha": "^10.0.10",
        "chai": "^4.5.0",
        "dotenv": "^16.3.1",
        "hardhat": "^2.19.4",
        "hardhat-deploy": "^0.11.45",
        "hardhat-gas-reporter": "^1.0.10",
        "nodemon": "^3.0.1",
        "solidity-coverage": "^0.8.16",
        "ts-node": "^10.9.2",
        "typechain": "^8.3.2",
        "typescript": "^5.8.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qbandev/stbchef.git"
    },
    "keywords": [
        "crypto",
        "trading",
        "bot",
        "ai",
        "ethereum",
        "linea",
        "pectra"
    ],
    "author": "qbandev",
    "license": "PolyForm-Strict-1.0.0"
}